# Test Environment Configuration
# This file contains test-specific environment variables

# Test Mode
LLM_KERNEL_ENV=test
LLM_KERNEL_DEBUG=false
LLM_KERNEL_LOGGING=false

# Test API Keys (non-functional, for testing only)
OPENAI_API_KEY=test-openai-key-not-real
ANTHROPIC_API_KEY=test-anthropic-key-not-real
GOOGLE_API_KEY=test-google-key-not-real

# Test Directories
LLM_KERNEL_CACHE_DIR=/tmp/llm_kernel_test_cache
LLM_KERNEL_CONFIG_DIR=/tmp/llm_kernel_test_config
LLM_KERNEL_LOG_FILE=/tmp/llm_kernel_test.log

# Test Model Configuration
DEFAULT_LLM_MODEL=gpt-4o
FALLBACK_LLM_MODEL=gpt-4o-mini

# Test Limits
MAX_CONTEXT_TOKENS=1000
MAX_CONTEXT_CELLS=10
REQUEST_TIMEOUT=5

# Disable External Services
DISABLE_MCP=true
DISABLE_OLLAMA=true
DISABLE_TELEMETRY=true

# Test Feature Flags
ENABLE_MOCK_RESPONSES=true
ENABLE_TEST_MODE=true
SKIP_API_VALIDATION=true